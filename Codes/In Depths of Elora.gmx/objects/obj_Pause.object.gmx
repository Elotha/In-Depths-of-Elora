    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Step

{
    var dir = (keyboard_check_pressed(vk_down) || keyboard_check_pressed(ord('S'))) - (keyboard_check_pressed(vk_up) || keyboard_check_pressed(ord('W')));
    if dir = -1
        {
        if selected_number = 0
            {
            selected[0] = 0;
            selected[selected_total-1] = 0.5;
            selected_number = selected_total-1;
            button_current = pause_menu[selected_number];
            }
        else
            {
            selected[selected_number--] = 0;
            selected[selected_number] = 0.5;
            button_current = pause_menu[selected_number];
            }
        }
    else if dir = 1
        {
        if selected_number = selected_total-1
            {
            selected[selected_total-1] = 0;
            selected[0] = 0.5;
            selected_number = 0;
            button_current = pause_menu[selected_number];
            }
        else
            {
            selected[selected_number++] = 0;
            selected[selected_number] = 0.5;
            button_current = pause_menu[selected_number];
            }
        }
    if keyboard_check_pressed(vk_enter)
        {
        switch(button_current)
            {
            case "RESUME":
                keyboard_key_press(vk_escape);
                break;
                
            case "SETTINGS":
                break;
                
            case "MAIN MENU":
                instance_destroy();
                scr_back_to_menu();
                room_goto(rm_Menu);
                break;
                
            }
        }
}
</string>
          </argument>
        </arguments>
      </action>
    </event>
