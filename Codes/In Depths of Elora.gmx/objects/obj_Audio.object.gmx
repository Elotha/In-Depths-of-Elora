        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Music &amp; Sounds

{
    var msc, snd;
    if room = rm_Menu with(obj_MainMenu) //Ana menüdeysek
        {
        if menu_current = "New Profile" || menu_current = "Controls" then exit;
        msc = control[controlkeys.music];
        snd = control[controlkeys.sounds];
        }
    else if instance_number(obj_Character) != 0 
        {
        msc = obj_Character.control[controlkeys.music];
        snd = obj_Character.control[controlkeys.sounds];
        }
    else exit;
    if keyboard_check_pressed(msc) //Muzik tuşuna bastık
        {
        if global.music_volume &gt; 0
            {
            global.music_volume_last = global.music_volume;
            global.music_volume = 0;
            scr_music();
            }
        else
            {
            global.music_volume = global.music_volume_last;
            scr_music();
            }
        ini_open(current_profile + "_config.ini");
        ini_write_real("Audio","Music",global.music_volume);
        ini_close();
        }
    if keyboard_check_pressed(snd)  //Sesler tuşuna bastık
        {
        if global.sounds_volume &gt; 0
            {
            global.sounds_volume_last = global.sounds_volume;
            global.sounds_volume = 0;
            }
        else
            {
            global.sounds_volume = global.sounds_volume_last;
            }
        if room = rm_Menu then with(obj_MainMenu) audio[SOUNDS_VOLUME] = "SOUND EFFECTS VOLUME: " + "&lt;" + string(global.sounds_volume) + "/10" + "&gt;";
        ini_open(current_profile + "_config.ini");
        ini_write_real("Audio","Sounds",global.sounds_volume);
        ini_close();
        }
}
</string>
          </argument>
        </arguments>
      </action>
