            <string>///Step

{
    if gravity != 0 { //Bloğa değme anı
        var Block = collision_rectangle(x-sprite_get_width(sprite_index)/2,y,x+sprite_get_width(sprite_index)/2,y+abs(sprite_height)+vspeed,obj_Block,false,true);
        if Block != noone {
            vspeed = 0;
            y += distance_to_object(Block);
            hspeed = image_xscale;
            Teleport = false;
            gravity = 0;
            image_speed = 1/6;
            }
        }
    
    if !Teleport { //Karakter tespiti
        var Character = collision_rectangle(x-sprite_get_width(sprite_index)/2+8,bbox_bottom,x+sprite_get_width(sprite_index)/2-8,y+DetectY,obj_Character,false,true);
        if Character != noone &amp;&amp; !Character.Invincibility &amp;&amp; Character.y &gt; y {
            Teleport = true;
            sprite_index = spr_MinyooTeleportation;
            image_speed = 1/3;
            image_index = 0;
            speed = 0;
            var Block = collision_line(x,y,x,y+24,obj_Block,false,true);
            var i = (Block.image_yscale*8)+16;
            DistStep = i/((1/image_speed)*image_number);
            exit;
            }
            
        var BlockLeft = collision_rectangle(x-sprite_get_width(sprite_index)/2,y,x+LeftX,y+24,obj_Block,false,true);
        var BlockRight = collision_rectangle(x+sprite_get_width(sprite_index)/2,y,x+RightX,y+24,obj_Block,false,true);
        if (BlockLeft != noone &amp;&amp; BlockRight = noone) {
            hspeed = -abs(hspeed);
            image_xscale = -abs(image_xscale);
            }
            
        if BlockLeft = noone &amp;&amp; BlockRight != noone {
            hspeed = abs(hspeed);
            image_xscale = abs(image_xscale);
            }
        
        var Coll2 = collision_line(x+((sprite_get_width(sprite_index)/2-8)*image_xscale),y-sprite_yoffset+16,x+((sprite_get_width(sprite_index)/2+16)*image_xscale),y-sprite_yoffset+16,obj_Block,false,true);
        if Coll2 != noone &amp;&amp; Coll2 != BlockLeft &amp;&amp; Coll2 != BlockRight {
            hspeed = -hspeed;
            image_xscale = -image_xscale;
            }
        }
}
</string>
