    <event eventtype="7" enumb="7">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Animation End

{
    switch(sprite_index) {
        case spr_CharacterDoor: //Kapıdan geçme animasyonu bitmişse
            if room_exists(room_next(room)) then room_goto_next();
            break;
            
        case spr_CharacterDeath: //Ölme animasyonu bitmişse
            if obj_HUD.Lives &gt; 0 { //Hala canımız var
                scr_Clear();
                if global.Checkpoint = -1 { //Checkpoint açmadık
                    x = obj_StartPoint.x;
                    y = obj_StartPoint.y;
                    }
                else { //Checkpoint açtık
                    x = global.Checkpoint.x+40;
                    y = global.Checkpoint.y+40;
                    }
                scr_ExistenceCheck(); //Objeleri checkpointte kaydedilen hale getir
                with(obj_HUD)  {
                    KeyNumber[GOLDEN] = KeyCheckpoint;
                    MagicEssence = MagicEssenceCheckpoint;
                    }
                scr_Reborn();
                }
            else game_restart(); //Canımız kalmamış
            break;
        }
        
    if MagicalInteraction != -1 { //Büyü görüşü modunda bir canavarın özelliğini kullanıyorsak
        switch(sprite_index) {
            case spr_MagicalMinyooTelUp: //Yukarı yükselmeye başlıyoruz
                sprite_index = spr_MagicalMinyooLines;
                mask_index = sprite_index;
                VSpeed = -FallingSpeed;
                if JumpCount &lt; JumpMax then JumpCount++;
                MinyooRangeCount = 0;
                MinyooGravityDir = -1;
                break;
                
            case spr_MagicalMinyooTelUp2: //İnce bir platformda oluştuk
                sprite_index = spr_MagicalMinyooWalk;
                mask_index = sprite_index;
                image_speed = 0;
                MovementPermission = true;
                break;
                
            case spr_MagicalMinyooTelDown: //Platformdan aşağı ışınlanmak üzereyiz
                if image_yscale = 1 {
                    y += MinyooBlockY+16;
                    sprite_index = spr_MagicalMinyooTelDown2;
                    mask_index = sprite_index;
                    }
                else {
                    y = MinyooBlockY-8;
                    sprite_index = spr_MagicalMinyooTelUp2;
                    mask_index = sprite_index;
                    image_yscale = 1;
                    depth = MONSTER;
                    }
                break;
                
            case spr_MagicalMinyooTelDown2: //Platformdan aşağı ışınlanmak üzereyiz
                sprite_index = spr_MagicalMinyooWalk;
                mask_index = sprite_index;
                VSpeed = FallingSpeed;
                break;
            }
        }
}
</string>
