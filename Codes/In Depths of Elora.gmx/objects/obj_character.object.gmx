        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Potion

{
    switch(other.sprite_index) {
        case spr_PotionHP_Big:
            scr_Potion(snd_PotionHP_Big,c_red,"+ Soul Power Points!");
            obj_HUD.SoulPower = obj_HUD.SoulPowerMax;
            scr_HUDInteract();
            break;
            
        case spr_PotionHP_Small:
            obj_HUD.SoulPower += 20;
            if obj_HUD.SoulPower &gt; obj_HUD.SoulPowerMax then obj_HUD.SoulPower = obj_HUD.SoulPowerMax;
            scr_Potion(snd_PotionHP_Big,c_red,"+ Soul Power Points!");
            scr_HUDInteract();
            break;
            
        case spr_PotionLight:
            with(obj_CharacterLight) {
                LightSizeNew += 150;
                alarm[0] = 1;
                LightPotion++;
                alarm[1] = room_speed*10;
                }
            scr_Potion(snd_PotionLight,c_yellow,"+ Light Radius Increased!");
            scr_HUDInteract();
            break;
            
        case spr_PotionResistance:
            scr_Potion(snd_PotionResistance,c_teal,"+ Resistance Increased!");
            switch(Resistance) {
                case 1:
                    Resistance = 0.75;
                    break;
                    
                case 0.75:
                    Resistance = 0.5;
                    break;
                    
                case 0.5:
                    Resistance = 0.25;
                    break;
                    
                case 0.25:
                    Resistance = 0;
                    break;
                }
            alarm[5] = room_speed*10;
            scr_HUDInteract();
            break;
            
        }
    destroy(other);
}
</string>
          </argument>
        </arguments>
      </action>
    </event>
